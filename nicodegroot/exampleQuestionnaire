form taxOfficeExample {
  "Integer with value 10"
    cQ1: integer = (10)
    
  "The integer from above + 6"
    cQ11: integer = (cQ1 + 6)
    
  "30.00 money times the above"
    cQ3: money = (30.00 * cQ11)
    
  "The 300.00 money times the second from above."
    cQ2: money = (cQ3 * cQ11)
    
    // Maybe a true or false check is cool? So no expression, just when a boolean is used directly. Then do a warning.
    
    "Toggle switch for if statement"
	  cQ9: boolean
    
    if (cQ9) {
    	"Just empty money"
    	  cQ4: money
    	  
    	"The empty money times 2"
    	  cQ5: money = (cQ4 * 2)
    	  
    	"The above times 2"
    	  cQ55: money = (cQ5 * 2)
    	  
    	"Just a boolean true"
    	  cQ6: boolean = (true)
    	  
    	"Just a string"
    	  cQ7: string = ("abc")
    }
}