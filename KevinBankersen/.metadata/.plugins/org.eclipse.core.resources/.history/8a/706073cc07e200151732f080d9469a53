package eu.bankersen.kevin.ql.context;

import eu.bankersen.kevin.ql.ast.Identifier;
import eu.bankersen.kevin.ql.ast.Literal;
import eu.bankersen.kevin.ql.ast.Variable;

public interface QLVisitor {
    
    void visit(Variable data);
    void visit(Identifier data);
    void visit(Literal data);
    void visit(And data);
    void visit(Eq data);
    void visit(GEq data);
    void visit(GT data);
    void visit(LEq data);
    void visit(LT data);
    void visit(NEq data);
    void visit(Not data);
    void visit(Or data);
    void visit(Add data);
    void visit(Div data);
    void visit(Mul data);
    void visit(Neg data);
    void visit(Pos data);
    void visit(Sub data);

}
