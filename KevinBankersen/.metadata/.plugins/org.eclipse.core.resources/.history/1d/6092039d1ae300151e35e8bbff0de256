
public class App {

    public static void main(String[] args) {
	Fruit[] fruits = new Fruit[]
		    { new Orange(), new Apple(), new Banana(),
		      new Banana(), new Banana(), new Orange() };

	FruitPartitioner partitioner = new FruitPartitioner();
	for (Fruit x : fruits){
	    x.Accept(partitioner);
	}
    }

}
