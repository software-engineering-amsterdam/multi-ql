package eu.bankersen.kevin.ql.typechecker.errors;

import eu.bankersen.kevin.ql.ast.stat.AbstractQuestion;
import eu.bankersen.kevin.ql.ast.type.Type;

public class AllreadyDeclaredError extends TypeCheckError {

    public AllreadyDeclaredError(AbstractQuestion o, Type original) {
	super(o.line(), String.format("The variable \"%s\" is already declared with type %s instead of %s!",o.name(), original, o.type()));
    }

}
