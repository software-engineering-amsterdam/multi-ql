package eu.bankersen.kevin.ql.ast;

public interface FullVisitor extends Visitor {
    
    void visit(NumberExpr o);

    void visit(Sub o);

    void visit(Add o);

    void visit(Div o);

    void visit(Mul o);

    void visit(Pos o);

    void visit(Neg o);
    
    void visit(BooleanExpr o);

    void visit(Or o);

    void visit(And o);

    void visit(Eq o);

    void visit(GEq o);

    void visit(GT o);

    void visit(LEq o);

    void visit(LT o);

    void visit(NEq o);

    void visit(Not o);

}
