package eu.bankersen.kevin.ql.ast.expr;

public class strLiteral extends strExpr{

    private final String value;

    public strLiteral(final String value) {
	this.value = value;
    }

    @Override
    public final String result(final SymbolTabel table) {
	return value;
    }

    @Override
    public final Boolean checkType(final SymbolTabel table) {
	return true;
    }
}
