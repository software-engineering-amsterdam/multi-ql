package eu.bankersen.kevin.ql.util.testing.widget;

import eu.bankersen.kevin.ql.ast.Type;
import eu.bankersen.kevin.ql.context.Symbol;

public class WidgetFactory {
    
    public Widget getWidget(Symbol data){
	
	switch (data.getType()) {
	case INTEGER : return new DropdownWidget(data);  
	case BOOLEAN : return new RadioButtonWidget(data);
	default : return new BoxWidget(data);
	}
    }

}
