package eu.bankersen.kevin.ql.util.testing.new1;

import eu.bankersen.kevin.ql.ast.Type;

public class BooleanType extends TypeObject {

    private final Type type = Type.BOOLEAN;

    @Override
    public Type getType() {
	return Type.BOOLEAN;
    }

    @Override
    public boolean compatible(TypeObject typeObject) {
	return sameType(typeObject);
    }
    
    @Override
    public boolean sameType(TypeObject typeObject) {
	return typeObject.getType().equals(type);
    }

}
