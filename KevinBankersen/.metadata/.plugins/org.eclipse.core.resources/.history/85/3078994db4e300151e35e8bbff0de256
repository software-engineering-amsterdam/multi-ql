package eu.bankersen.kevin.ql.typechecker.errors;

import java.util.Set;

import eu.bankersen.kevin.ql.typechecker.errors.TypeCheckError;

public class ContextBuildException extends Exception {
    
    private final Set<TypeCheckError> exceptions;
    
    public ContextBuildException(final Set<TypeCheckError> errors) {
	this.exceptions = errors;
    }
    
    public final Set<TypeCheckError> getErrors() {
	return exceptions;
    }

}
