package eu.bankersen.kevin.ql.ast.expr.math;

import eu.bankersen.kevin.ql.ast.expr.Expr;
import eu.bankersen.kevin.ql.ast.expr.IntegerExpr;
import eu.bankersen.kevin.ql.context.Context;
import eu.bankersen.kevin.ql.context.EvaluateExeption;

public class Sub extends IntegerExpr {

    public Sub(Expr lhs, Expr rhs, int line) {
	super(lhs, rhs, line);
    }

    @Override
    public Integer eval(Context context) throws EvaluateExeption {
	return (Integer) lhs().eval(context) - (Integer) rhs().eval(context);
    }

}
