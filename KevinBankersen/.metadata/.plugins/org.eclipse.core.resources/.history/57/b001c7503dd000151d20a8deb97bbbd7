package eu.bankersen.kevin.ql.ast.form;

public class Form {

	private String id;
	public Block body;

	public Form(String id, Block body) {
		this.id = id;
		this.body = body;
	}

	
	@Override
	public String toString() {
		StringBuilder sb;

		sb = new StringBuilder();
		sb.append("Form: " + id);
		sb.append("\n");

		return sb.toString();
	}
}

