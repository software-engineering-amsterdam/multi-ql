package eu.bankersen.kevin.ql.typechecker;

import java.util.Set;

import eu.bankersen.kevin.ql.typechecker.symboltable.SymbolTable;
import eu.bankersen.kevin.ql.typechecker.warnings.TypeCheckWarning;

public class TypeCheckWarnings extends Exception {
    
    private final Set<TypeCheckWarning> warnings;
    private final SymbolTable symbolTable;
    
    public TypeCheckWarnings(final Set<TypeCheckWarning> warnings, SymbolTable symboltable) {
	this.warnings = warnings;
    }
    
    public final Set<TypeCheckWarning> getWarnings() {
	return warnings;
    }

}
