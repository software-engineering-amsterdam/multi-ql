package eu.bankersen.kevin.ql.ast.expr.math;

import eu.bankersen.kevin.ql.ast.expr.Expr;
import eu.bankersen.kevin.ql.ast.expr.IntegerExpr;
import eu.bankersen.kevin.ql.context.EvaluateExeption;

public class Mul extends IntegerExpr {

    public Mul(final Expr lhs, final Expr rhs,  final int line) {
	super(lhs, rhs, line);
    }

    @Override
    public final Integer eval() throws EvaluateExeption {
	return (Integer) lhs().eval() * (Integer) rhs().eval();
    }


}
