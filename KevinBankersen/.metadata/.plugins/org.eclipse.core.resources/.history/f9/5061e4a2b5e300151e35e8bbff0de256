package eu.bankersen.kevin.ql.gui.dialog;

import java.util.Set;

import eu.bankersen.kevin.ql.typechecker.warnings.TypeCheckWarning;

public class WarningDialog {
    private final boolean accept;
    
    public WarningDialog(Set<TypeCheckWarning> warnings) {
	StringBuilder sb = new StringBuilder();
	warnings.forEach(warning -> sb.append(warning + "\n"));
	accept = showWarning(sb.toString());
    }
    
    private boolean showWarning(String message) {
	return true;
    }
    
    public boolean userAccept() {
	return accept;
    }

}
