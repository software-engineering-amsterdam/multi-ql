package eu.bankersen.kevin.ql.util.testing.widget;

import eu.bankersen.kevin.ql.ast.Type;

public class WidgetFactory {
    
    public Widget getWidget(Type type){
	
	switch (type) {
	case INTEGER : return new IntegerWidget();  
	case BOOLEAN : return new RadioButtonWidget();
	default : return new StringWidget();
	}
    }

}
