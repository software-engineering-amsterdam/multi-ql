package eu.bankersen.kevin.ql.ast.type.value;

public abstract class QLObject {

    public abstract boolean isSimilar(QLObject type);

    public boolean isCompatible(QLObject type) {
	return isSimilar(type);
    }
    
    abstract String toString(String value);

    public Object parseValue(String value) {
	return null;
    }

    public Object parseValue(Double value) {
	return null;
    }

    public Object parseValue(Boolean value) {
	return null;
    }
}
