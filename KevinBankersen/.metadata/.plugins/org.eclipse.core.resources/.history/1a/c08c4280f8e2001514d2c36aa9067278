package eu.bankersen.kevin.ql.ast.stat;

import eu.bankersen.kevin.ql.ast.type.Type;
import eu.bankersen.kevin.ql.context.Context;

public class QuestionStatement extends Question {

    public QuestionStatement(String name, String text, Type type, int line) {
	super(name, text, type, null, false, line);
    }
    
    @Override
    public Context checkType(Context context) {
	return context.evaluate(this);
    }
}
