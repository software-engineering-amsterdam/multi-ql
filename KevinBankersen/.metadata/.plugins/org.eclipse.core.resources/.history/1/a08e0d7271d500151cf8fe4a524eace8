package eu.bankersen.kevin.ql.ast.expr.math;

import eu.bankersen.kevin.ql.ast.expr.Expr;
import eu.bankersen.kevin.ql.ast.expr.IntegerExpr;
import eu.bankersen.kevin.ql.symboltable.Context;

public class Pos extends IntegerExpr {

    private final Expr expr;

    public Pos(final Expr expr) {
	this.expr = expr;
    }

    @Override
    public final Object eval(final Context context) {
	return Math.abs((Integer) expr.eval(context));
    }

}
