package eu.bankersen.kevin.ql.ast.expr;

public class NEq extends boolExpr {

	private final intExpr lhs;
	private final intExpr rhs;

	public NEq(Expr lhs, Expr rhs) {
		this.lhs = (intExpr) lhs;
		this.rhs = (intExpr) rhs;
	}

	@Override
	public Boolean interpret(interpExpr expr) {
		return lhs.interpret(expr) != rhs.interpret(expr);
	}

}
