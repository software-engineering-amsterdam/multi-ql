package eu.bankersen.kevin.ql.ast.expr;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

import eu.bankersen.kevin.ql.ast.var.Variable;

public class SymbolTabel2 {
    
    static public void changeString(String x){
	table.setString(x);
    }
    
    static public void addStrings(String x){
	table.addString(x);
    }
    
    static public String getContent(){
	return table.getString();
    }
    
    static public String getContents(){
	
	table.test2.add("jaja");
	
	return table.getStrings();
    }
    
    private static class table {
	
	private static String test;
	
	private static List<String> test2 = new ArrayList<String>();
	
	protected static void setString(String y) {
	    table.test = y;
	}
	
	protected static void addString(String y) {
	    test2.add(y);
	}
	
	protected static String getString() {
	    return test;
	}
	
	protected static String getStrings() {
	    return test2.toString();
	}
	
    }
}
