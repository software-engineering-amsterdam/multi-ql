package eu.bankersen.kevin.ql.util.testing2;

public class main {

    public static void main(String[] args) {

	Parent parent = new Parent();
	
	SmallChild a = new SmallChild("jan");
	parent.addChild(a);
	
	BigChild b = new BigChild("pieter");
	parent.addChild(b);
	
	
	for (Children x : parent.getChildren()) {
	    BooleanQuestion q = x;
	    System.out.println(q.getValue());
	}
    }
    
    public <Any> Any test(BooleanQuestion b){
	return((Any)((BooleanQuestion)(b)));
	}

}
