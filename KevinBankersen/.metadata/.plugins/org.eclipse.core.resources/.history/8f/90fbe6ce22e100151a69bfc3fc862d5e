package eu.bankersen.kevin.ql.util.testing.new1;

import eu.bankersen.kevin.ql.ast.Type;

public abstract class TypeObject {
    
    public final Type type;
    
    TypeObject(Type type){
	this.type = type;
    }
    
    public abstract Type getType();
    
    public abstract boolean compatible(TypeObject type);
    
    public abstract boolean sameType(TypeObject type);

}
