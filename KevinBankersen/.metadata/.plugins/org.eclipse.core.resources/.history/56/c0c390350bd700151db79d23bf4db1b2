package eu.bankersen.kevin.ql.util.testing;

import eu.bankersen.kevin.ql.ast.Type;
import eu.bankersen.kevin.ql.context.Context2;

public class tester {

    public static void main(String[] args) {

	Context2 context = Context2.getInstance();
	
	context.addSymbol("test", Type.BOOLEAN);
	
	if (context.getSymbol("test").getType().equals(Type.BOOLEAN)){
	    System.out.println("YES!");
	}
	
    }

}
