package eu.bankersen.kevin.ql.ast.expr;

public class intLiteral extends intExpr {

	private final Integer value;
	
	public intLiteral(final Integer value) {
		this.value = value;
	}

	@Override
	public final String toString(){
		return value.toString();
	}

	@Override
	public final Integer result(final SymbolTabel table) {
		return value;
	}

	@Override
	public final Boolean checkType(final SymbolTabel table) {
		return true;
	}
	
}
