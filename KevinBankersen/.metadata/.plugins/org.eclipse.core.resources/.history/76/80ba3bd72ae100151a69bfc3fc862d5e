package eu.bankersen.kevin.ql.util.testing.new1;

import eu.bankersen.kevin.ql.ast.Type;

public abstract class TypeObject {
    
    public abstract Type getType();
    
    public abstract boolean compatible(TypeObject typeObject);
    
    public abstract boolean sameType(TypeObject typeObject) {
	return typeObject.getType().equals(this.getType());
    }
    
    public Object parseValue(String value) {
	return Type.EMPTY;
    }
    
    public Object parseValue(Integer value) {
	return Type.EMPTY;
    }
    
    public Object parseValue(Boolean value) {
	return Type.EMPTY;
    }

}
