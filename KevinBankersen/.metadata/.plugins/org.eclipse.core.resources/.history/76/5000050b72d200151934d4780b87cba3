package eu.bankersen.kevin.ql.ast.expr;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

import eu.bankersen.kevin.ql.ast.var.Type;

public class SymbolTabel2 {
    
    private static HashMap<String, SymbolTableObject> table = new HashMap<String, SymbolTableObject>();

    public static void addVariable(final String name, final Type type) {
	
	table.put(name, new SymbolTableObject(name, type));
    }
    
    public static Type getType (final String name){
	return table.get(name).type;
    }
    
    public static String getContents() {
	
	return table.toString();
    }
}
