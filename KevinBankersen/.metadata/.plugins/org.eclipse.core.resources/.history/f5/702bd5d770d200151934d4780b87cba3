package eu.bankersen.kevin.ql.ast.expr;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

import eu.bankersen.kevin.ql.ast.var.Type;
import eu.bankersen.kevin.ql.ast.var.Variable;

public class SymbolTabel2 {
    
    private static List<SymbolTableObject> table;
    
    public SymbolTabel2() {
	table = new ArrayList<SymbolTableObject>();
    }
    

    public static void addVariable(String name, Type type){
	table.add(SymbolTableObject(name, type));
    }
    
    static public String getContents(){
	
	return table.toString();
    }
}
