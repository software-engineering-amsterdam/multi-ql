package eu.bankersen.kevin.ql.ast.expr;

import java.util.HashMap;

import eu.bankersen.kevin.ql.ast.var.Variable;

public class SymbolTabel2 {

    private HashMap<String, Variable> table;

    public SymbolTabel2() {
	table = new HashMap<String, Variable>();
    }

    public final void setValue(final String key, final Variable value) {
	table.put(key, value);
    }

    public final Variable getValue(final String key) {
	return table.get(key);
    }
    
    private static class table{
	
    }
}
