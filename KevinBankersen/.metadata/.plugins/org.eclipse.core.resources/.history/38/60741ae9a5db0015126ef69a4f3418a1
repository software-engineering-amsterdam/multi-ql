package eu.bankersen.kevin.ql.util.testing2;

public class main {

    public static void main(String[] args) {

	Parent parent = new Parent();
	
	SmallChild a = new SmallChild("jan");
	parent.addChild(a);
	
	BigChild b = new BigChild("pieter");
	parent.addChild(b);
	
	
	parent.getChildren().forEach(child -> test(child));

    }
    
    private void test(BooleanQuestion x) {
	System.out.println(x.getValue());
    }

}
