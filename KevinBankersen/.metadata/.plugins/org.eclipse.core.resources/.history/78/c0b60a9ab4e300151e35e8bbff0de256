package eu.bankersen.kevin.ql.typechecker;

import java.util.Set;

import eu.bankersen.kevin.ql.typechecker.errors.TypeCheckError;

public class TypeCheckWarnings extends Exception {
    
    private final Set<TypeCheckWarning> warnings;
    
    public TypeCheckWarnings(final Set<TypeCheckWarning> warnings) {
	this.warnings = warnings;
    }
    
    public final Set<TypeCheckWarning> getWarnings() {
	return warnings;
    }

}
