package eu.bankersen.kevin.ql.ast.expr;

import eu.bankersen.kevin.ql.ast.var.Type;

public class Literal extends Expr {

    private final Object value;
    private final Type type;
    
    Literal(final Object value, final Type type) {
	this.value = value;
	this.type = type;
    }
    
    @Override
    public final Object result(final SymbolTabel table) {
	return value;
    }

    @Override
    public final Boolean checkType(final SymbolTabel table) {
	// TODO Auto-generated method stub
	return null;
    }

    @Override
    public final Type getType() {
	return type;
    }
    
    

}
